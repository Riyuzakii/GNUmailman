#!/home/light/dev/mailman/venv3/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'falcon','console_scripts','falcon-print-routes'
__requires__ = 'falcon'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('falcon', 'console_scripts', 'falcon-print-routes')()
    )
